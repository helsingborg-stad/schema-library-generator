{
    "name": "helsingborg-stad/schema-library-generator",
    "description": "A fluent builder Schema.org types and ld+json generator",
    "license": "MIT",
    "keywords": [
        "helsingborg",
        "schema-org"
    ],
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Tom Witkowski",
            "email": "dev.gummibeer@gmail.com",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/spatie/schema-org",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "graham-campbell/analyzer": "^4.2",
        "illuminate/collections": "^8.62.0",
        "league/flysystem": "^2.3.0 || ^3.0",
        "symfony/console": "^5.3.7 || 6.0",
        "twig/twig": "^3.3.3"
    },
    "require-dev": {
        "helsingborg-stad/phpcs": "^0.3.5",
        "pestphp/pest": "^1.21"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\SchemaOrg\\Generator\\": "generator"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\SchemaOrg\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "scripts": {
        "lint": "vendor/bin/phpcs",
        "fix": "vendor/bin/phpcbf",
        "generate": "@php ./generate.php",
        "test": "@php vendor/bin/pest"
    },
    "bin": [
        "generate"
    ],
    "version": "0.1.4"
}